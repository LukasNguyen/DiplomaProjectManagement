<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>Gentelella Alela! | </title>
    <link href="~/Assets/admin/libs/gentelella/vendors/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
    <link href="~/Assets/admin/libs/gentelella/vendors/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <link href="~/Assets/admin/libs/gentelella/vendors/nprogress/nprogress.css" rel="stylesheet" />
    <link href="~/Assets/admin/libs/gentelella/vendors/animate.css/animate.css" rel="stylesheet" />
    <link href="~/Assets/admin/libs/gentelella/build/css/custom.css" rel="stylesheet" />
    <link href="~/Assets/admin/libs/toastr/toastr.css" rel="stylesheet" />
    @RenderSection("Styles", false)
</head>
<body class="nav-md">
    <div class="container body">
        <div class="main_container">
            <div class="col-md-3 left_col">
                <div class="left_col scroll-view">
                    <div class="navbar nav_title" style="border: 0;">
                        <a href="index.html" class="site_title"><i class="fa fa-paw"></i> <span style="font-size: 80%">ĐỒ ÁN TỐT NGHIỆP</span></a>
                    </div>

                    <div class="clearfix"></div>

                    <!-- menu profile quick info -->
                    <div class="profile clearfix">
                        <div class="profile_pic">
                            <img src="/Assets/admin/img/user2-160x160.jpg" alt="..." class="img-circle profile_img">
                        </div>
                        <div class="profile_info left-side">
                            <span>Xin chào,</span>
                            <h2 style="font-size: 92%">@User.Identity.Name</h2>
                        </div>
                    </div>
                    <!-- /menu profile quick info -->

                    <br />

                    <!-- sidebar menu -->
                    @Html.Partial("_SideBarPartial")
                    <!-- /sidebar menu -->
                    <!-- /menu footer buttons -->
                </div>
            </div>

            <!-- top navigation -->
            <div class="top_nav">
                <div class="nav_menu">
                    <nav>
                        <div class="nav toggle">
                            <a id="menu_toggle"><i class="fa fa-bars"></i></a>
                        </div>

                        <ul class="nav navbar-nav navbar-right">
                            <li class="">
                                @Html.Partial("_UserInfomationLogin_Partial")
                            </li>
                        </ul>
                    </nav>
                </div>
            </div>
            <!-- /top navigation -->
            <!-- page content -->
            <div class="right_col" role="main">
                <div class="">
                    @RenderBody()
                </div>
            </div>
            <!-- /page content -->
            <!-- footer content -->
            <footer>
                <div class="pull-right">
                </div>
                <div class="clearfix"></div>
            </footer>
            <!-- /footer content -->
        </div>
    </div>

    <script src="~/Assets/admin/libs/gentelella/vendors/jquery/dist/jquery.js"></script>
    <script src="~/Assets/admin/libs/bootstrap/dist/js/bootstrap.js"></script>
    <script src="~/Assets/admin/libs/gentelella/vendors/fastclick/lib/fastclick.js"></script>
    <script src="~/Assets/admin/libs/gentelella/vendors/nprogress/nprogress.js"></script>
    <script src="~/Assets/admin/libs/gentelella/vendors/jquery-sparkline/dist/jquery.sparkline.min.js"></script>
    <script src="~/Assets/admin/libs/twbs-pagination/jquery.twbsPagination.js"></script>
    <script src="~/Assets/admin/libs/gentelella/vendors/moment/moment.js"></script>
    <script src="~/Assets/admin/libs/gentelella/vendors/bootstrap-daterangepicker/daterangepicker.js"></script>
    <script src="~/Assets/admin/libs/mustache.js/mustache.js"></script>
    <script src="~/Assets/admin/libs/toastr/toastr.js"></script>
    <script src="~/Assets/admin/libs/gentelella/build/js/custom.js"></script>
    <script src="~/Assets/client/shared/common.js"></script>
    @RenderSection("Scripts", false)
    @Html.Partial("_DisplayMessagePartial")
</body>
</html>